# 프로덕션 환경 설정
NODE_ENV=production
PORT=3000

# 데이터베이스 설정 (DB 서버와 동일한 서버에서 실행)
# localhost로 연결 (같은 서버에 PostgreSQL이 설치되어 있는 경우)
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=wonki
DB_PASSWORD=wonki
DB_NAME=ocean_story

# JWT 설정 (실제 운영 환경에서는 강력한 시크릿 키 사용)
JWT_SECRET=change-this-to-strong-secret-key-in-production
JWT_EXPIRES_IN=1h

# CORS 설정 (프론트엔드 도메인)
CORS_ORIGIN=http://your-frontend-domain.com
